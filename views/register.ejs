<%- include('partials/header') %>
<form action="/user/register" method="post" class="part">
  <label for="name">Jméno a příjmení</label> 
  <input type="text" name="name" required /> 

  <label for="phone">Telefon (bez předvolby)</label> 
  <input type="tel" name="phone" minlength="9" maxlength="9" /> 

  <label for="username">Uživatelské jméno</label> 
  <input type="text" name="username" required /> 

  <label for="password">Heslo</label> 
  <input type="password" name="password" required />

  <label for="password2">Heslo</label> 
  <input type="password" name="password2" required />
  <input type="submit" value="Zaregistrovat se" />
</form>
<%- include('partials/footer') %>
